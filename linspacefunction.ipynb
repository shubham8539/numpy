      "source": [
        "def func(a,b,nr):\n",
        "    st=(b-a)/nr\n",
        "    lst=[a]\n",
        "    for i in range(0,nr-1):\n",
        "        lst.append(lst[i]+st)\n",
        "    return(lst)"
      ],
